%!PS-Adobe-3.0
%%BoundingBox: 150 150 400 400

/mm { 72 25.4 div mul } bind def

/hole-r 3.2 mm 2 div def
/laser-w 55 mm 2 div def
/outer-disk 64 mm 2 div def
/mount-r 58 mm 2 div def

0 setlinewidth

/box {
  /h exch def
  /w exch def

  w 0 rlineto
  0 h rlineto
  w neg 0 rlineto
  0 h neg rlineto
  closepath
  stroke
} def

100 mm 100 mm translate

outer-disk 0 moveto 0 0 outer-disk 0 360 arc

laser-w 8 mm sub 0 moveto
laser-w 8 mm sub 0 hole-r 0 360 arc

laser-w neg 7 mm add 0 moveto
laser-w neg 7 mm add 0 hole-r 0 360 arc

stroke

%laser-w neg -16 mm moveto 55 mm 32 mm box

gsave
-5 rotate  % let's give more.
0 mount-r moveto
0 0 mount-r 90 150 arc
0 0 mount-r hole-r 2 mul sub 150 90 arcn
closepath
stroke

gsave
180 rotate
0 mount-r moveto
0 0 mount-r 90 150 arc
0 0 mount-r hole-r 2 mul sub 150 90 arcn
closepath
stroke
grestore

grestore

showpage
